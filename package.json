{
  "name": "express-typescript-seed",
  "version": "1.0.0",
  "private": false,
  "author": "Kevin Kolz <kckolz@gmail.com>",
  "scripts": {
    "start": "yarn run build && yarn run serve",
    "test": "yarn run build && rimraf tests/sqlite.db && NODE_ENV=test jest --forceExit --verbose dist/tests/*.test.js",
    "build": "yarn run clean && tsc",
    "serve": "tsc-watch --outDir ./dist --onSuccess \"node ./dist/app.js\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "yarn run build && yarn run serve-debug",
    "serve-debug": "tsc-watch --outDir ./dist --onSuccess \"node --inspect=9222 --trace-warnings ./dist/app.js\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "aws-sdk": "^2.175.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.14.1",
    "compression": "^1.7.1",
    "connect-ensure-login": "^0.1.1",
    "connect-roles": "^3.1.2",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.4.2",
    "express": "^4.13.3",
    "express-limiter": "^1.6.1",
    "jsonwebtoken": "^8.1.0",
    "method-override": "^2.3.5",
    "moment": "^2.19.3",
    "morgan": "^1.9.1",
    "multer": "^1.3.0",
    "oauth2orize": "^1.2.0",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-http-oauth": "^0.1.3",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.1.2",
    "passport-oauth2-client-password": "^0.1.2",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.2",
    "sequelize": "^4.22.15",
    "sequelize-typescript": "^0.5.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.40",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.16.2",
    "@types/compression": "0.0.33",
    "@types/errorhandler": "0.0.30",
    "@types/express": "^4.0.35",
    "@types/jest": "^22.0.1",
    "@types/jsonwebtoken": "^7.2.4",
    "@types/lodash": "^4.14.63",
    "@types/morgan": "^1.7.32",
    "@types/multer": "^1.3.6",
    "@types/node": "^8.0.53",
    "@types/passport": "^0.3.3",
    "@types/request": "0.0.45",
    "@types/sequelize": "^4.0.79",
    "@types/supertest": "^2.0.4",
    "jest": "^22.0.6",
    "passport-debug": "^0.1.15",
    "sqlite3": "^3.1.1",
    "supertest": "^3.0.0",
    "ts-node": "^3.3.0",
    "tsc-watch": "^1.0.13",
    "tslint": "^5.0.0",
    "typescript": "^2.6.1"
  }
}
